<Application x:Class="InteractiveShravyaProfile.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:InteractiveShravyaProfile"
             xmlns:locator ="clr-namespace:InteractiveShravyaProfile.IOC.ViewModelLocator"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <locator:ViewModelLocator x:Key="ViewModelLocator"/>
        </ResourceDictionary>

    </Application.Resources>
</Application>
